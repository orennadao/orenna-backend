{
  "buildCommand": "pnpm --filter @orenna/web build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://orenna-backend-production.up.railway.app/api/$1"
    }
  ]
}