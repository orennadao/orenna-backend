{
  "name": "@orenna/db",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:push": "dotenv -e ../../.env -- prisma db push",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev --name init",
    "prisma:seed": "dotenv -e ../../.env -- tsx prisma/seed.ts",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio --schema=./prisma/schema.prisma",
    "test": "vitest run"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.22.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^1.6.1"
  }
}